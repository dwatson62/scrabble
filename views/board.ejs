<table class="tg">
  <% for (x = 0; x < 15; x ++) { %>
    <tr>
      <% for (y = 0; y < 15; y ++) { %>
        <td class="tg-031e">
          <div>
            <img ng-class='scrbCtrl.showBoardTiles(<%= x %>, <%= y %>)' id='img<%= x %>-<%= y %>' ng-src='/images/tiles/{{ scrbCtrl.tile(<%= x %>, <%= y %>) }}.png' ng-click='scrbCtrl.selectTile(<%= x %>, <%= y %>)'>
          </div>
        </td>
      <% } %>
    </tr>
  <% } %>
</table>
